<?xml version="1.0"?>
<interface>
  <requires lib="gtk+" version="2.16"/>
  <!-- interface-naming-policy toplevel-contextual -->
  <object class="GtkMenu" id="menu">
    <property name="visible">True</property>
    <signal name="hide" handler="on_menu_hide"/>
    <child>
      <object class="GtkImageMenuItem" id="menuitem5">
        <property name="label">gtk-open</property>
        <property name="visible">True</property>
        <property name="use_underline">True</property>
        <property name="use_stock">True</property>
        <property name="accel_group">accelgroup1</property>
        <signal name="activate" handler="on_menuitem5_activate"/>
      </object>
    </child>
    <child>
      <object class="GtkMenuItem" id="menuitem10">
        <property name="visible">True</property>
        <property name="label" translatable="yes">Open _Recent</property>
        <property name="use_underline">True</property>
      </object>
    </child>
    <child>
      <object class="GtkImageMenuItem" id="menuitem11">
        <property name="label" translatable="yes">_Next Photo</property>
        <property name="visible">True</property>
        <property name="use_underline">True</property>
        <property name="image">image1</property>
        <property name="use_stock">False</property>
        <signal name="activate" handler="on_next_photo"/>
      </object>
    </child>
    <child>
      <object class="GtkSeparatorMenuItem" id="menuitem9">
        <property name="visible">True</property>
      </object>
    </child>
    <child>
      <object class="GtkCheckMenuItem" id="menuitem8">
        <property name="visible">True</property>
        <property name="label" translatable="yes">_Full Screen</property>
        <property name="use_underline">True</property>
        <accelerator key="F11" signal="activate"/>
        <signal name="toggled" handler="on_menuitem8_toggled"/>
      </object>
    </child>
    <child>
      <object class="GtkCheckMenuItem" id="menuitem6">
        <property name="visible">True</property>
        <property name="label" translatable="yes">Fi_x Photo Frame</property>
        <property name="use_underline">True</property>
        <signal name="toggled" handler="on_menuitem6_toggled"/>
      </object>
    </child>
    <child>
      <object class="GtkSeparatorMenuItem" id="menuitem7">
        <property name="visible">True</property>
      </object>
    </child>
    <child>
      <object class="GtkImageMenuItem" id="menuitem1">
        <property name="label">gtk-preferences</property>
        <property name="visible">True</property>
        <property name="use_underline">True</property>
        <property name="use_stock">True</property>
        <property name="accel_group">accelgroup1</property>
        <signal name="activate" handler="on_prefs"/>
      </object>
    </child>
    <child>
      <object class="GtkImageMenuItem" id="menu_help">
        <property name="label">gtk-help</property>
        <property name="visible">True</property>
        <property name="use_underline">True</property>
        <property name="use_stock">True</property>
        <signal name="activate" handler="on_help"/>
      </object>
    </child>
    <child>
      <object class="GtkImageMenuItem" id="menuitem2">
        <property name="label">gtk-about</property>
        <property name="visible">True</property>
        <property name="use_underline">True</property>
        <property name="use_stock">True</property>
        <property name="accel_group">accelgroup1</property>
        <signal name="activate" handler="on_about"/>
      </object>
    </child>
    <child>
      <object class="GtkSeparatorMenuItem" id="menuitem3">
        <property name="visible">True</property>
      </object>
    </child>
    <child>
      <object class="GtkImageMenuItem" id="menuitem4">
        <property name="label">gtk-quit</property>
        <property name="visible">True</property>
        <property name="use_underline">True</property>
        <property name="use_stock">True</property>
        <property name="accel_group">accelgroup1</property>
        <signal name="activate" handler="on_quit"/>
      </object>
    </child>
  </object>
  <object class="GtkImage" id="image1">
    <property name="visible">True</property>
    <property name="stock">gtk-go-forward</property>
    <property name="icon-size">1</property>
  </object>
  <object class="GtkAccelGroup" id="accelgroup1"/>
</interface>
